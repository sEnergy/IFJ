<?php //Program 1: Vypocet faktorialu (iterativne)

//Hlavni telo programu

$var1 = "Marcel Fiala";
$var2 = "Fiala";

$x = put_string("\nJdeme testovat find_string:\n");
$result = find_string($var1, $var2);
$x = put_string("Result:", $result, "\n");

$x = put_string("\nJdeme testovat get_substring:\n");
$result = get_substring($var1, 0, 6);
$x = put_string("Result:", $result, "\n");


